{
  "expo": {
    "name": "FYP",
    "slug": "FYP",
    "scheme": "FYP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },

      "package": "com.fyp",
      "config": {
        "googleMaps": 
            {
              "apiKey": "AIzaSyD6DVLho-QJOqaxGKZ9pDQLYuDkvxlTyuw"
            },
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "FORGROUND_SERVICE", "INTERNET", "ACCESS_BACKGROUND_LOCATION"]
          }  
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
